[defaults]
inventory = hosts
# Change path whereever mitogen is installed
strategy_plugins = ~/.virtualenvs/infra/lib/python3.9/site-packages/ansible_mitogen/plugins/strategy
strategy = mitogen_linear
# Use MacOS Keychain for vault password
# https://github.com/andrasmaroy/dotfiles/blob/master/bin/Darwin/ansible-vault-pass
vault_password_file = ~/.bin/Darwin/ansible-vault-pass
